{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.16",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.5",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.3",
        "symfony/asset": "7.3.*",
        "symfony/asset-mapper": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/doctrine-messenger": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/expression-language": "7.3.*",
        "symfony/flex": "^2",
        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.3.*",
        "symfony/process": "7.3.*",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/stimulus-bundle": "^2.30",
        "symfony/string": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/ux-turbo": "^2.30",
        "symfony/validator": "7.3.*",
        "symfony/web-link": "7.3.*",
        "symfony/yaml": "7.3.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpro/grumphp-shim": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
      "start" : [
        "Composer\\Config::disableProcessTimeout",
        "symfony serve"
      ],
      "fix:csfixer": [
        "php-cs-fixer fix"
      ],
      "test:csfixer" : [
        "php-cs-fixer fix --dry-run"
      ],
      "test:phpstan" : [
        "phpstan analyse"
      ],
      "test:twig" : [
        "twig-cs-fixer lint"
      ],
      "fix:twig": [
        "twig-cs-fixer lint --fix"
      ],
      "test:yaml" : [
        "bin/console lint:yaml config"
      ],
      "test": [
        "@test:csfixer",
        "@test:phpstan",
        "@test:twig",
        "@test:yaml"
      ],
      "fix" : [
        "@fix:csfixer",
        "@fix:twig"
      ],
      "db": [
        "bin/console doctrine:database:drop --force --if-exists",
        "bin/console doctrine:database:create",
        "bin/console doctrine:migrations:migrate --no-interaction",
        "bin/console doctrine:fixtures:load --no-interaction"
      ],

      "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "scripts-descriptions": {
      "start" : "Run Web server on symfony",
      "fix:csfixer": "Fix PHP code with PHP CS Fixer",
      "test:csfixer" : "Test PHP code with PHP CS Fixer",
      "fix:twig": "Fix Twig code with twig CS Fixer",
      "test:twig" : "Test Twig code with twig CS Fixer",
      "test" : "Run all tests",
      "fix": "Run all fix"
    },

  "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.1",
        "php-cs-fixer/shim": "^3.88",
        "phpro/grumphp-shim": "^2.15",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-beberlei-assert": "^2.0",
        "phpunit/phpunit": "^12.3",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/debug-bundle": "7.3.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/stopwatch": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*",
        "vincentlanglet/twig-cs-fixer": "^3.10",
        "zenstruck/foundry": "^2.7"
    }
}
